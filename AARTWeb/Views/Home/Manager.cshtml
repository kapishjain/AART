                        alert(e.responseJSON.info);
                        $("#grid").data("kendoGrid").dataSource.read();
                    }
                }
            },
            schema: {
                model: {
                    id: "pro_doc_id",
                    fields: {
                        pro_doc_id: { type: "int", editable: false },
                        product_description: { type: "string", editable: false, nullable: true, validation: { required: true } },
